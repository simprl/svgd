{
  "name": "@svgd/cli",
  "version": "1.0.31",
  "description": "Command-line utility for generating constants from SVG assets",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "svgd": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run tests/stories/stories.test.ts",
    "docs": "tsx scripts/codeDoc && tsx scripts/useCasesDoc",
    "lint": "eslint"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "devDependencies": {
    "@svgd/mocks": "*",
    "@types/node": "^18.19.71",
    "codools": "^0.2.17",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@svgd/utils": "^0.1.33",
    "commander": "^12.1.0"
  },
  "keywords": [
    "svg",
    "svgd",
    "cli",
    "icons",
    "svg icons",
    "svg sprite",
    "svg path d",
    "svgo",
    "icon-generator",
    "constants"
  ]
}
